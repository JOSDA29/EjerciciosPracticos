/*Escribe un método sin parámetros y sin retorno que imprima en pantalla tu nombre completo.*/

import javax.swing.JOptionPane;

public class NombreCompleto {

	public static void main(String[] args) {
		nombre();
	}
	
	public static void nombre() {
		JOptionPane.showMessageDialog(null, "Mi nombre es: \n Joseph David Quintero Vargas" , "INFO" ,JOptionPane.INFORMATION_MESSAGE);
	}

}
