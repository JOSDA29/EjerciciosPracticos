import javax.swing.JOptionPane;

public class NumeroMayorDiez {

	public static void main(String[] args) {
		int numero = Integer.parseInt(JOptionPane.showInputDialog("Digite cualquier numero enetero"));
	}

	public static void numeroMayor (int numero) {
		String mensaje;
		if (numero>10) {
			mensaje= "El numero: "+numero+"\n Es mayor a 10 ";
			JOptionPane.showMessageDialog(null, mensaje , "INFO" ,JOptionPane.INFORMATION_MESSAGE);
		}
	}
}
